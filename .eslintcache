ist,\n      wishlistFlag,\n      authorName,\n      bookPrice,\n      addToCart,\n    } = this.props;\n  \n  return (\n    <div className=\"bookContainer\" key={keyID}>\n      <div className=\"bookCover\">\n        <img className=\"bookCoverImage\" src={imageSrc} alt={bookTitle} />\n        <button\n          className=\"toWishlist\"\n          // srOnly='true'\n          onClick={() => {\n            handleWishlist(book, keyID);\n          }}\n        >\n          {wishlistFlag === false\n            ? `Add to Wishlist ♥️`\n            : `Remove from Wishlist`}\n        </button>\n      </div>\n\n      <div className=\"bookDetails\">\n        <h3>{bookTitle}</h3>\n        <p className=\"authorName\">{authorName}</p>\n        <p className=\"bookPrice\">\n          $ {bookPrice}\n          {/* If inventory is less than <=5 and more than 1 then display number of books available  */}\n          {book.inventory <= 15 && book.inventory >= 1 ? (\n            <span className=\"inventoryLeft\">{book.inventory} available</span>\n          ) : null}\n        </p>\n      </div>\n      <div className=\"addTo\">\n        {\n          // If inventory is 0 then display out of stock\n          book.inventory <= 0 ? (\n            <p className=\"outOfStock\">Out of stock </p>\n          ) : (\n            <button\n              className=\"addToCart\"\n              onClick={() => {\n                addToCart(book, keyID);\n              }}\n            >\n              Add to Cart\n            </button>\n          )\n        }\n        <button\n          className=\"quickWishlist\"\n          title='Add to Wishlist'\n          onClick={() => handleWishlist(book, keyID)}\n          onMouseEnter={() => this.wishlistOnHover()}\n          onMouseLeave={() => this.wishlistOnHover()}\n        >\n          {wishlistFlag === false ? (\n            <img\n              src={this.state.wishlistState}\n              alt=\"wishlist button\"\n              className=\"heartShapeEmptyWishlistIcon\"\n            />\n          ) : (\n            <img\n              src={redheart}\n              alt=\"wishlist button\"\n              className=\"heartShapeRedWishlistIcon\"\n            />\n          )}\n        </button>\n      </div>\n      \n    </div>\n  );\n}\n\n}\n\n\nexport default Book;\n","/Users/schandrasekhar/juno/sites/projects/project5/project-five/src/Components/wishlistDisplay.js",[],"/Users/schandrasekhar/juno/sites/projects/project5/project-five/src/Components/WishlistDisplay.js",[],["73","74"],"/Users/schandrasekhar/juno/sites/projects/project5/project-five/src/Components/Modal.js",[],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"75","replacedBy":"79"},{"ruleId":"77","replacedBy":"80"},{"ruleId":"75","replacedBy":"81"},{"ruleId":"77","replacedBy":"82"},{"ruleId":"75","replacedBy":"83"},{"ruleId":"77","replacedBy":"84"},{"ruleId":"85","severity":1,"message":"86","line":4,"column":8,"nodeType":"87","messageId":"88","endLine":4,"endColumn":13},{"ruleId":"75","replacedBy":"89"},{"ruleId":"77","replacedBy":"90"},"no-native-reassign",["91"],"no-negated-in-lhs",["92"],["91"],["92"],["91"],["92"],["91"],["92"],"no-unused-vars","'Modal' is defined but never used.","Identifier","unusedVar",["91"],["92"],"no-global-assign","no-unsafe-negation"]